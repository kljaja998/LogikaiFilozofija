<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.gimvukts.kljaja.logikaifilozofija.MainActivityFragment"
    tools:showIn="@layout/activity_main">

    <Button
        android:text="Logika - Pojmovi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bttn_logikaPojmovi"
        android:onClick="bttn_logikaPojmovi_onClick"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Filozofija - Pojmovi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bttn_filozofijaPojmovi"
        android:onClick="bttn_filozofijaPojmovi_onClick"
        android:layout_below="@+id/bttn_logikaPojmovi"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Filozofija - Citati"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bttn_filozofijaCitati"
        android:onClick="bttn_filozofijaCitati_onClick"
        android:layout_below="@+id/bttn_filozofijaPojmovi"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Mladen KljajiÄ‡ IV-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
